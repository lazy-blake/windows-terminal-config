        "source": "Windows.Terminal.VisualStudio"
      },
      {
        "commandline": "%WINDIR%\\System32\\WindowsPowerShell\\v1.0\\powershell.exe -ExecutionPolicy ByPass -NoExit -Command \"& '$env:USERPROFILE\\anaconda3\\shell\\condabin\\conda-hook.ps1' ; conda activate '$env:USERPROFILE\\anaconda3' \"",
        "name": "Anaconda PowerShell Prompt (anaconda3)",
        "icon": "$env:USERPROFILE\\anaconda3\\Menu\\anaconda_powershell_prompt.ico",
        "startingDirectory": "$env:USERPROFILE"
      },
      {
        "commandline": "%WINDIR%\\System32\\cmd.exe \"/K\" $env:USERPROFILE\\anaconda3\\Scripts\\activate.bat $env:USERPROFILE\\anaconda3",
        "name": "Anaconda Prompt (anaconda3)",
        "icon": "$env:USERPROFILE\\anaconda3\\Menu\\anaconda_prompt.ico",
        "startingDirectory": "$env:USERPROFILE"
      }
    ]
